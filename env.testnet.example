# Hyperliquid Testnet Configuration
# Copy this to .env for testnet testing

# Database
DATABASE_URL=postgresql://localhost/maxxit_testnet

# Hyperliquid Testnet Mode
HYPERLIQUID_TESTNET=true
HYPERLIQUID_SERVICE_URL=http://localhost:5001

# Agent Wallet Encryption (generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
AGENT_WALLET_ENCRYPTION_KEY=your_32_byte_hex_key_here

# Use Arbitrum Sepolia for testing
USE_SEPOLIA=true
SEPOLIA_RPC=https://sepolia-rollup.arbitrum.io/rpc

# Executor (for module operations)
EXECUTOR_PRIVATE_KEY=your_test_executor_private_key

# Module Address (testnet)
MODULE_ADDRESS=your_testnet_module_address

# Twitter API (same for testnet/mainnet)
TWITTER_BEARER_TOKEN=your_twitter_bearer_token

# OpenAI (same for testnet/mainnet)
OPENAI_API_KEY=your_openai_key

# Optional: Testnet-specific settings
NODE_ENV=development
LOG_LEVEL=debug

